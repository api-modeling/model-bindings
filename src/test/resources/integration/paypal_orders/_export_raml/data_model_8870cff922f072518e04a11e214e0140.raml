#%RAML 1.0 Library
types:
  MerchantsCommonComponentsSpecification-v1-schema-shipping_options_request-json:
    description: Includes options presented by the payee/merchant to the payer to ship or pick up their item(s). Also includes the shipping options elected by the payer.
    properties:
      id:
        type: string
      label:
        type: string
      attribute1:
        type: string
      amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
  MerchantsCommonComponentsSpecification-v1-schema-authorization-json:
    description: The authorized payment transaction.
    type:
      - MerchantsCommonComponentsSpecification-v1-schema-authorization_status-json
      - MerchantsCommonComponentsSpecification-v1-schema-activity_timestamps-json
    properties:
      id:
        type: string
      invoice_id:
        type: string
      expiration_time:
        type: string
      amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
  seller_payable_breakdown:
    description: The breakdown of the refund.
    properties:
      gross_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      paypal_fee: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      net_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      platform_fees:
        items: MerchantsCommonComponentsSpecification-v1-schema-platform_fee-json
      total_refunded_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
  purchase_unit_request-json:
    description: The purchase unit request. Includes required information for the payment contract.
    properties:
      reference_id:
        type: string
      description:
        type: string
      custom_id:
        type: string
      invoice_id:
        type: string
      soft_descriptor:
        type: string
      amount: MerchantsCommonComponentsSpecification-v1-schema-amount_with_breakdown-json
      payee: MerchantsCommonComponentsSpecification-v1-schema-payee-json
      payment_instruction: MerchantsCommonComponentsSpecification-v1-schema-payment_instruction-json
      items:
        items: MerchantsCommonComponentsSpecification-v1-schema-item-json
      shipping: MerchantsCommonComponentsSpecification-v1-schema-shipping_detail-json
  MerchantsCommonComponentsSpecification-v1-schema-refund-json:
    description: The refund information.
    type:
      - MerchantsCommonComponentsSpecification-v1-schema-refund_status-json
      - MerchantsCommonComponentsSpecification-v1-schema-activity_timestamps-json
    properties:
      id:
        type: string
      invoice_id:
        type: string
      note_to_payer:
        type: string
      amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      seller_payable_breakdown: seller_payable_breakdown
      links:
        items: MCCS-v1-schema-CC-v3-json-openapi-2-link_description-json
  MerchantsCommonComponentsSpecification-v1-schema-exchange_rate-json:
    description: The exchange rate that determines the amount to convert from one currency to another currency.
    properties:
      source_currency:
        type: string
      target_currency:
        type: string
      value:
        type: string
  MCCS-v1-schema-CC-v3-json-openapi-2-name-json:
    description: The name of the party.
    properties:
      prefix:
        type: string
      given_name:
        type: string
      surname:
        type: string
      middle_name:
        type: string
      suffix:
        type: string
      alternate_full_name:
        type: string
      full_name:
        type: string
  MCCS-v1-schema-CC-v3-json-openapi-2-patch-json:
    description: The JSON patch object to apply partial updates to resources.
    properties:
      op:
        type: string
      path:
        type: string
      from:
        type: string
      value: value
  MerchantsCommonComponentsSpecification-v1-schema-payment_method-json:
    description: The customer and merchant payment preferences.
    properties:
      payer_selected:
        type: string
      payee_preferred:
        type: string
  MCCS-v1-schema-CC-v3-json-openapi-2-phone-json:
    description: The phone number, in its canonical international [E.164 numbering plan format](https://www.itu.int/rec/T-REC-E.164/en).
    properties:
      country_code:
        type: string
      national_number:
        type: string
      extension_number:
        type: string
  MerchantsCommonComponentsSpecification-v1-schema-amount_with_breakdown-json:
    description: The total order amount with an optional breakdown that provides details, such as the total item amount, total tax amount, shipping, handling, insurance, and discounts, if any.<br/>If you specify `amount.breakdown`, the amount equals `item_total` plus `tax_total` plus `shipping` plus `handling` plus `insurance` minus `shipping_discount`.<br/>The amount must be a positive number. For listed of supported currencies and decimal precision, see the PayPal REST APIs <a href="/docs/integration/direct/rest/currency-codes/">Currency Codes</a>.
    type: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
    properties: {}
  MerchantsCommonComponentsSpecification-v1-schema-authorization_status-json:
    description: The status fields for an authorized payment.
    properties:
      status:
        type: string
  MCCS-v1-schema-CC-v3-json-openapi-2-error_details-json:
    description: The error details. Required for client-side `4XX` errors.
    properties:
      field:
        type: string
      value:
        type: string
      location:
        type: string
      issue:
        type: string
      description:
        type: string
  MCCS-v1-schema-CC-v3-json-openapi-2-address_portable-json:
    description: "The portable international postal address. Maps to [AddressValidationMetadata](https://github.com/googlei18n/libaddressinput/wiki/AddressValidationMetadata) and HTML 5.1 [Autofilling form controls: the autocomplete attribute](https://www.w3.org/TR/html51/sec-forms.html#autofilling-form-controls-the-autocomplete-attribute)."
    properties:
      address_line_1:
        type: string
      address_line_2:
        type: string
      address_line_3:
        type: string
      admin_area_4:
        type: string
      admin_area_3:
        type: string
      admin_area_2:
        type: string
      admin_area_1:
        type: string
      postal_code:
        type: string
      address_details: address_details
  patternProperties:
    description: The pattern properties.
    type: any
  items:
    description: A link.
    type: any
  additionalItems:
    description: Any additional items.
    type: any
  order-json:
    description: The order details.
    type: MerchantsCommonComponentsSpecification-v1-schema-activity_timestamps-json
  MerchantsCommonComponentsSpecification-v1-schema-card-json:
    description: The payment card to use to fund a payment. Can be a credit or debit card.
    properties:
      id:
        type: string
      name:
        type: string
      number:
        type: string
      security_code:
        type: string
      last_digits:
        type: string
      billing_address: MCCS-v1-schema-CC-v3-json-openapi-2-address_portable-json
  MerchantsCommonComponentsSpecification-v1-schema-payment_source-json:
    description: The payment source definition.
    type: any
  application_context-json:
    description: Customizes the payer experience during the approval process for the payment with PayPal.<blockquote><strong>Note:</strong> Partners and Marketplaces might have <code>brand_name</code> and <code>shipping_preference</code> configured during partner account setup that will override the request values.</blockquote>
    properties:
      brand_name:
        type: string
      landing_page:
        type: string
      shipping_preference:
        type: string
      user_action:
        type: string
      return_url:
        type: string
      cancel_url:
        type: string
      payment_token:
        type: string
  purchase_unit-json:
    description: The purchase unit details. Used to capture required information for the payment contract.
    properties:
      reference_id:
        type: string
      description:
        type: string
      custom_id:
        type: string
      invoice_id:
        type: string
      id:
        type: string
      soft_descriptor:
        type: string
  MerchantsCommonComponentsSpecification-v1-schema-payee_base-json:
    description: The details for the merchant who receives the funds and fulfills the order. The merchant is also known as the payee.
    properties:
      email_address:
        type: string
      merchant_id:
        type: string
      client_id:
        type: string
  MerchantsCommonComponentsSpecification-v1-schema-payee_displayable-json:
    description: The merchant information. The merchant is also known as the payee. Appears to the customer in checkout, transactions, email receipts, and transaction history.
    properties:
      business_email:
        type: string
      brand_name:
        type: string
      business_phone: MCCS-v1-schema-CC-v3-json-openapi-2-phone-json
  status_details:
    description: The details of the captured payment status.
    properties:
      reason:
        type: string
  authorize_request-json:
    description: Authorize an order.
    type: order_action_request-json
  properties:
    type: any
  MerchantsCommonComponentsSpecification-v1-schema-item-json:
    description: The details for the items to be purchased.
    properties:
      name:
        type: string
      quantity:
        type: string
      description:
        type: string
      sku:
        type: string
      category:
        type: string
      unit_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      tax: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
  address_details:
    description: The non-portable additional address details that are sometimes needed for compliance, risk, or other scenarios where fine-grain address information might be needed. Not portable with common third party and opensource. Redundant with core fields. For example, `address_portable.address_line_1` is usually a combination of `address_details.street_number` and `street_name` and `street_type`.
    properties:
      street_number:
        type: string
      street_name:
        type: string
      street_type:
        type: string
      delivery_service:
        type: string
      building_name:
        type: string
      sub_building:
        type: string
  seller_receivable_breakdown:
    description: The detailed breakdown of the captured payment.
    properties:
      gross_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      paypal_fee: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      net_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      receivable_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      platform_fees:
        items: MerchantsCommonComponentsSpecification-v1-schema-platform_fee-json
  MerchantsCommonComponentsSpecification-v1-schema-refund_status-json:
    description: The refund status.
    properties:
      status:
        type: string
      status_details: status_details
  payment_collection-json:
    description: The collection of payments, or transactions, for a purchase unit in an order. For example, authorized payments, captured payments, and refunds.
    type: any
  MCCS-v1-schema-CC-v3-json-openapi-2-link_schema-json:
    description: The request data or link target.
    properties:
      fragmentResolution:
        type: string
      pathStart:
        type: string
      additionalItems: additionalItems
      dependencies: dependencies
      items: items
      definitions: definitions
      patternProperties: patternProperties
      properties: properties
      allOf:
        items: items
      anyOf:
        items: items
      oneOf:
        items: items
      not: not
      links:
        items: items
      media: media
  MerchantsCommonComponentsSpecification-v1-schema-platform_fee-json:
    description: The platform or partner fee, commission, or brokerage fee that is associated with the transaction. Not a separate or isolated transaction leg from the external perspective. The platform fee is limited in scope and is always associated with the original payment for the purchase unit.
    properties:
      amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      payee: MerchantsCommonComponentsSpecification-v1-schema-payee_base-json
  not:
    type: any
  MerchantsCommonComponentsSpecification-v1-schema-payee-json:
    description: The merchant who receives the funds and fulfills the order. The merchant is also known as the payee.
    type: MerchantsCommonComponentsSpecification-v1-schema-payee_base-json
    properties: {}
  media:
    description: The media type and context-encoding scheme.
    properties:
      attribute2:
        type: string
      binaryEncoding:
        type: string
  MerchantsCommonComponentsSpecification-v1-schema-token-json:
    description: The tokenized payment source to fund a payment.
    properties:
      id:
        type: string
      attribute3:
        type: string
  MCCS-v1-schema-CC-v3-json-openapi-2-link_description-json:
    description: The request-related [HATEOAS link](/docs/api/overview/#hateoas-links) information.
    properties:
      href:
        type: string
      rel:
        type: string
      method:
        type: string
      title:
        type: string
      mediaType:
        type: string
      encType:
        type: string
  order_action_request-json:
    description: Completes an action for an order.
    type: any
  MerchantsCommonComponentsSpecification-v1-schema-shipping_detail-json:
    description: The shipping details.
    properties:
      name: MCCS-v1-schema-CC-v3-json-openapi-2-name-json
      address: MCCS-v1-schema-CC-v3-json-openapi-2-address_portable-json
  definitions:
    type: any
  MerchantsCommonComponentsSpecification-v1-schema-net_amount_breakdown_item-json:
    description: The net amount. Returned when the currency of the refund is different from the currency of the PayPal account where the merchant holds their funds.
    properties:
      payable_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      converted_amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      exchange_rate: MerchantsCommonComponentsSpecification-v1-schema-exchange_rate-json
  MerchantsCommonComponentsSpecification-v1-schema-payment_instruction-json:
    description: Any additional payment instructions for PayPal for Partner customers. Enables features for partners and marketplaces, such as delayed disbursement and collection of a platform fee. Applies during order creation for captured payments or during capture of authorized payments.
    type: any
  MerchantsCommonComponentsSpecification-v1-schema-activity_timestamps-json:
    description: The date and time stamps that are common to authorized payment, captured payment, and refund transactions.
    type: any
  order_request-json:
    description: The order request details.
    properties:
      intent:
        type: string
      payer: MerchantsCommonComponentsSpecification-v1-schema-payer-json
  MerchantsCommonComponentsSpecification-v1-schema-phone_with_type-json:
    description: The phone information.
    type: any
  MerchantsCommonComponentsSpecification-v1-schema-capture_status-json:
    description: The status of a captured payment.
    properties:
      status:
        type: string
      status_details: status_details
  value:
    description: The value to apply. The `remove` operation does not require a value.
    type: any
  dependencies:
    type: any
  MerchantsCommonComponentsSpecification-v1-schema-amount_breakdown-json:
    description: The breakdown of the amount. Breakdown provides details such as total item amount, total tax amount, shipping, handling, insurance, and discounts, if any.
    properties:
      item_total: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      shipping: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      handling: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      tax_total: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      insurance: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      shipping_discount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
  MerchantsCommonComponentsSpecification-v1-schema-tax_info-json:
    description: The tax ID of the customer. The customer is also known as the payer. Both `tax_id` and `tax_id_type` are required.
    properties:
      tax_id:
        type: string
      tax_id_type:
        type: string
  MCCS-v1-schema-CC-v3-json-openapi-2-money-json:
    description: The currency and amount for a financial transaction, such as a balance or payment due.
    properties:
      value:
        type: string
  MCCS-v1-schema-CC-v3-json-openapi-2-error-json:
    description: The error details.
    properties:
      name:
        type: string
      message:
        type: string
      debug_id:
        type: string
      information_link:
        type: string
      links:
        items: MCCS-v1-schema-CC-v3-json-openapi-2-link_description-json
  MerchantsCommonComponentsSpecification-v1-schema-seller_protection-json:
    description: The level of protection offered as defined by [PayPal Seller Protection for Merchants](https://www.paypal.com/us/webapps/mpp/security/seller-protection).
    properties:
      status:
        type: string
      dispute_categories:
        items:
          type: string
  MerchantsCommonComponentsSpecification-v1-schema-capture-json:
    description: A captured payment.
    type:
      - MerchantsCommonComponentsSpecification-v1-schema-capture_status-json
      - MerchantsCommonComponentsSpecification-v1-schema-activity_timestamps-json
    properties:
      id:
        type: string
      invoice_id:
        type: string
      final_capture:
        type: boolean
      disbursement_mode:
        type: string
      amount: MCCS-v1-schema-CC-v3-json-openapi-2-money-json
      seller_protection: MerchantsCommonComponentsSpecification-v1-schema-seller_protection-json
      seller_receivable_breakdown: seller_receivable_breakdown
      links:
        items: MCCS-v1-schema-CC-v3-json-openapi-2-link_description-json
  MerchantsCommonComponentsSpecification-v1-schema-payer-json:
    description: The customer who approves and pays for the order. The customer is also known as the payer.
    type: any
